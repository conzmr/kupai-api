{
  "name": "Branch",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "properties": {
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "alias": {
      "type": "string"
    },
    "logo": {
      "type": "string"
    },
    "address": {
      "type": "string",
      "required": true
    },
    "geolocation": {
      "type": "geopoint",
      "required": true
    },
    "city": {
      "type": "string"
    },
    "country": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "restaurant": {
      "type": "belongsTo",
      "model": "RestaurantUser",
      "foreignKey": ""
    },
    "promotions": {
      "type": "hasAndBelongsToMany",
      "model": "Promotion",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "coupons": {
      "type": "hasAndBelongsToMany",
      "model": "Coupon",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
