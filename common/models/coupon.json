{
  "name": "Coupon",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "properties": {
    "createdAt": {
      "type": "date",
      "required": true,
      "default": "$now"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "discountType": {
      "type": "string",
      "required": true
    },
    "value": {
      "type": "number",
      "required": true
    },
    "details": {
      "type": "string"
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "expirationDate": {
      "type": "date"
    },
    "code": {
      "type": "string",
      "required": true
    },
    "reusable": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "available": {
      "type": "number",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "branches": {
      "type": "hasAndBelongsToMany",
      "model": "Branch",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
